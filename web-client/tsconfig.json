  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false, 
    "noEmit": false,
    "jsx": "react-jsx",
    "baseUrl": "src"
  },
  "include": ["src"]
}
